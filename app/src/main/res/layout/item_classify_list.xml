<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_evaluate_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/trans_23px"
        android:text="@string/app_name"
        android:textSize="@dimen/text_trans_27px"
        android:textColor="@color/moreText"
        android:layout_marginBottom="@dimen/trans_15px"
        />

    <TextView
        android:id="@+id/tv_evaluate_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_evaluate_type"
        android:gravity="center"
        android:paddingBottom="@dimen/trans_20px"
        android:text="@string/app_name"
        android:textSize="@dimen/text_trans_27px"
        android:textColor="@color/moreText"/>

</RelativeLayout>
